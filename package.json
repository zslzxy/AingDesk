{
  "name": "aingdesk",
  "version": "1.2.3",
  "description": "Simple and easy-to-use AI big model deployment and usage software",
  "productName": "AingDesk",
  "main": "./dist/server/app.js",
  "type": "commonjs",
  "scripts": {
    "dev": "npm run build-server && npm run start",
    "build": "npm run build-frontend && npm run build-server",
    "start": "node start.js",
    "start-prod": "node dist/server/app.js",
    "dev-server": "ts-node server/app.ts",
    "build-frontend": "cd frontend && npm run build && cd .. && cp -r frontend/dist public/",
    "build-server": "tsc --project tsconfig.server.json",
    "clean": "rm -rf dist public/dist",
    "test": "vitest"
  },
  "repository": "https://github.com/aingdesk/aingdesk",
  "keywords": [
    "AingDesk",
